#include <MIDI.h>//Default MIDI SETUPMIDI_CREATE_DEFAULT_INSTANCE();void setup() {    MIDI.begin();  MIDI.setHandleNoteOn(handleNoteOn); //Wenn eine Note gespielt werden soll wird diese Funktion aufgerufen  MIDI.setHandleNoteOff(handleNoteOff);//Wenn die Note nicht mehr gespielt wird diese  }void loop() {  MIDI.read();}//Wird aufgerufen wenn eine Note empfangen wirdvoid handleNoteOn(byte kanal, byte note, byte lautstaerke){  delay(10);  Serial.begin(9600);  Serial.println("Note soll gespielt werden");  Serial.print("Kanal: ");  Serial.print(kanal, DEC);  Serial.print("Note: ");  Serial.print(note % 12, DEC);  Serial.print("Lautstaerke: ");  Serial.println(lautstaerke, DEC);  delay(10);  Serial.begin(31250);}//Wird aufgerufen wenn die Note nicht mehr gespielt werden sollvoid handleNoteOff(byte kanal, byte note, byte lautstaerke){  delay(10);  Serial.begin(9600);  Serial.println("Note soll gestoppt werdn");  Serial.print("Kanal: ");  Serial.print(kanal, DEC);  Serial.print("Note: ");  Serial.print(note % 12, DEC);  Serial.print("Lautstaerke: ");  Serial.println(lautstaerke, DEC);  delay(10);  Serial.begin(31250);}